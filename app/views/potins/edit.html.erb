<%= form_tag url_for(action: 'update'), method: "put" do %>

    <%= label_tag 'Title' %>
    <%= text_field_tag 'title', @potin.title %>
    <br/> <br/>
  
    <%= label_tag 'content' %>
    <%= text_area_tag 'content', @potin.content %>
    <br/> <br/>
  
    <%= submit_tag 'Update post!' %>
  
<% end %>